#include <iostream>

using namespace std;

void PrintNumPattern(int num1, int num2, int rnCnt, int origNum){
   cout << num1 << " ";
   cout << " runCount: " << rnCnt << "\tnum1: " << num1 << " \torigNum " << origNum << endl;
   if(rnCnt == -origNum){
   }
   else if(rnCnt <= 0){
      PrintNumPattern(num1 + num2, num2, rnCnt - 1, origNum);
   }
   else if(rnCnt > 0){
      PrintNumPattern(num1 - num2, num2, rnCnt - 1, origNum);
   }
}

void PrintNumPattern(int num1, int num2, int rnCnt, int origNum);

int main() {
   int firstNum;
   int secondNum;
    
   cin >> firstNum;
   cin >> secondNum;
   int rnCnt = (firstNum/secondNum)+1;
   PrintNumPattern(firstNum, secondNum, rnCnt, rnCnt);
   
   return 0;
}
